import java.util.*;
public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int d1 = sc.nextInt(), c1 = sc.nextInt();
    int d2 = sc.nextInt(), c2 = sc.nextInt();
    int totalCents = (d1 + d2) * 100 + c1 + c2;
    System.out.println((totalCents / 100) + " \n" + (totalCents % 100));
  }
}
