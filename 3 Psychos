import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        double house=(a+c)/2.0;
        double house2=(b+d)/2.0;
        System.out.printf("Arun's house is located at(%.1f,%.1f)%n", house, house2);
    
        
    }
}
